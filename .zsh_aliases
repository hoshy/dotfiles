alias gpa='git fetch && git push origin develop && git push origin master && git push --tags'
alias gtl='git describe --tags `git rev-list --tags --max-count=1`'
alias gbc='git branch --merged | egrep -v "^\*|master|develop" | xargs -n 1 git branch -d'
alias ll='ls -alh'
alias gz='pigz'
alias gzu='pigz -d'
alias s3ls='s3cmd ls'
alias s3lsr='s3cmd ls -r'
alias s3get='s3cmd get'
alias s3getr='s3cmd get -r'
alias s3put='s3cmd put'
alias s3putr='s3cmd put -r'
alias s3putenc='s3cmd -e put'
alias s3putencr='s3cmd -e put -r'
alias tf='tail -f'
alias tfp='terraform remote pull && terraform get && terraform plan -module-depth=10'
alias tfa='terraform remote pull && terraform get && terraform apply'
alias cb='pbcopy'
alias lsbigs='ls -1Rhs | sed -e "s/^ *//" | grep "^[0-9]" | sort -hr | head -n20'
alias dc='docker-compose'
